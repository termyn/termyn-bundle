{
    "name": "termyn/termyn-bundle",
    "description": "Integration of the TERMYN toolkit into Symfony 5 and higher",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edo Barnas",
            "email": "iam@edobarnas.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Termyn\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Termyn\\Bundle\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "termyn/cqrs": "dev-master",
        "termyn/date-time": "dev-main",
        "termyn/ddd": "dev-master",
        "termyn/id": "dev-main",
        "symfony/framework-bundle": "5.*|6.*"
    },
    "require-dev": {
        "phpstan/phpstan": "1.9.*",
        "phpstan/phpstan-phpunit": "1.3.*",
        "phpunit/phpunit": "9.5.*",
        "symplify/easy-coding-standard": "11.1.*"
    }
}
